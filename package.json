{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Typescript: Node.js Api",
  "main": "index.js",
  "scripts": {
    "build": "yarn tsc",
    "start": "yarn build && node dist/src/index.js",
    "dev": "ts-node-dev --transpile-only --respawn --ignore-watch node_modules src/index.ts",

    "test:functional": "jest --projects ./tests --runInBand"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Yuri Rennan",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^16.10.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "jest": "^27.2.4",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "module-alias": "^2.2.2"
  }
}
